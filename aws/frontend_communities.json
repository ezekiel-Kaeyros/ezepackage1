{
    "family": "communitie_frontend",
    "containerDefinitions": [
        {
            "name": "communitie_frontend",
            "image": "ezekielkaeyros/eze_communities:latest",
            "cpu": 512,
            "memory": 1024,
            "memoryReservation": 1024,
            "portMappings": [
                {
                    "name": "communitie_frontend-3002-tcp",
                    "containerPort": 3000,
                    "hostPort": 0,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "mountPoints": [],
            "volumesFrom": [],
            "dockerLabels": {
                "traefik.enable": "true",
                "traefik.http.routers.communities.rule": "Host(`communities.eze.wiki`)"
            },
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/communitie_frontend",
                    "awslogs-region": "eu-central-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "systemControls": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::108165532263:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::108165532263:role/ecsTaskExecutionRole",
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "512",
    "memory": "1024",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}
