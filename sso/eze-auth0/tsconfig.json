{
  "compilerOptions": {
    "types": [
      "express-session"
    ],
    "typeRoots": [
      "node_modules/@types",
      "src/@types"
    ],
    "esModuleInterop": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "target": "ES2020",
    "moduleResolution": "nodenext"
  },
  "include": [
    "src/**/*.ts",
    "types.d.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
